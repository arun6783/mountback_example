{
  "name": "mountebackdemo",
  "version": "1.0.0",
  "description": "Simple walkthrough of mounteback",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "start:dev": "cross-env FORCE_COLOR=1 npm-run-all -p mb:start start",
    "mb:start": "cross-env NODE_OPTIONS=\"--require dotenv/config\" npm run mb:start:internal",
    "mb:start:internal": "cross-env-shell \"mb start --port $MOUNTEBANK_PORT --pidfile mb.$MOUNTEBANK_PORT.pid --allowInjection --localOnly true --configfile ./imposters/index.ejs --debug --loglevel $MOUNTEBANK_LOG_LEVEL\""
    },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.1"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "dotenv-webpack": "^8.0.0",
    "hpropagate": "^1.0.1",
    "mountebank": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-bootstrap": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "proxy": "http://localhost:3002"
}
